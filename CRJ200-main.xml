							<property>sim/model/start-idling</property>
						</binding>
					</item>
					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>
								aircraft.livery.dialog.open();
							</script>
						</binding>
					</item>
					<item>
						<label>Views</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>CRJViews</dialog-name>
						</binding>
					</item>
					<item>
						<label>EICAS control panel</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>eicas</dialog-name>
