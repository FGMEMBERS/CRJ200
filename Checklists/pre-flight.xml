<?xml version="1.0" encoding="UTF-8" ?>
<?xml-stylesheet type="text/xsl" href="checklist.xsl"?>

<PropertyList>
	<title>Pre-flight</title>
	<page>
		<item>
			<name>Parking Brake</name>
			<value>CHECK SET</value>
			<condition>
				<equals>
					<property>/controls/gear/brake-parking</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/gear/brake-parking</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Landing gear lever</name>
			<value>CHECK DOWN</value>
			<marker>
				<x-m>-13.9790</x-m>
				<y-m>0.0339</y-m>
				<z-m>-1.0876</z-m>
				<scale>3.8760</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/gear/gear-down</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/gear/gear-down</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Throttle</name>
			<value>CUT OFF</value>
			<marker>
				<x-m>-13.6286</x-m>
				<y-m>0</y-m>
				<z-m>-1.1909</z-m>
				<scale>3.8760</scale>
			</marker>
			<condition>
				<and>
					<property>/controls/engines/engine[0]/cutoff</property>
					<property>/controls/engines/engine[1]/cutoff</property>
				</and>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/engines/engine[0]/cutoff</property>
				<value>1</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/controls/engines/engine[1]/cutoff</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Flaps</name>
			<value>UP</value>
			<marker>
				<x-m>-13.7286</x-m>
				<y-m>0.1760</y-m>
				<z-m>-1.1909</z-m>
				<scale>3.8760</scale>
			</marker>
			<condition>
				<equals>
					<property>/surface-positions/flap-pos-norm</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/flight/flaps</property>
				<value>0</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.flapsDown(0);</script>
			</binding>
		</item>
		<item>
			<name>Battery Master</name>
			<value>ON</value>
			<view>
				<heading-offset-deg>321.1</heading-offset-deg>
				<pitch-offset-deg>28.2</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.5</x-offset-m>
				<y-offset-m>-0.5</y-offset-m>
				<z-offset-m>-15.0</z-offset-m>
				<field-of-view>55.0</field-of-view>
			</view>
			<marker>
				<x-m>-13.4088</x-m>
				<y-m>-0.1843</y-m>
				<z-m>-0.1273</z-m>
				<scale>1.4628</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/electric/battery-switch</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/electric/battery-switch</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Ground power</name>
			<value>SELECT</value>
			<view>
				<heading-offset-deg>321.1</heading-offset-deg>
				<pitch-offset-deg>28.2</pitch-offset-deg>
				<roll-offset-deg>0.0</roll-offset-deg>
				<x-offset-m>-0.5</x-offset-m>
				<y-offset-m>-0.5</y-offset-m>
				<z-offset-m>-15.0</z-offset-m>
				<field-of-view>55.0</field-of-view>
			</view>
			<marker>
				<x-m>-13.3997</x-m>
				<y-m>-0.1140</y-m>
				<z-m>-0.115</z-m>
				<scale>1.1488</scale>
			</marker>
			<condition>
				<property>/controls/electric/ac-service-selected</property>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/electric/ac-service-selected</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Internal lights</name>
			<value>as required</value>
		</item>
		<item>
			<name>Nav lights</name>
			<value>ON</value>
			<marker>
				<x-m>-13.6575</x-m>
				<y-m>-0.2298</y-m>
				<z-m>-0.2372</z-m>
				<scale>1.0000</scale>
			</marker>
			<condition>
				<equals>
					<property>/controls/lighting/nav-lights</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/lighting/nav-lights</property>
				<value>1</value>
			</binding>
		</item>
		<!--item>
			<name>Avionics Master</name>
			<value>ON</value>
		</item-->

		<item>
			<name>Fuel Quantity</name>
			<value>CHECK</value>
			<binding>
				<command>nasal</command>
				<script>gui.showWeightDialog()</script>
			</binding>
		</item>
		<item>
			<name>Request Fuel</name>
			<value>AS REQUIRED</value>
		</item>

		<item>
			<name>Passenger signs</name>
			<value>ON</value>
			<marker>
				<x-m>-13.7121</x-m>
				<y-m>0.0992</y-m>
				<z-m>-0.3107</z-m>
				<scale>2.0909</scale>
			</marker>
			<condition>
				<and>
					<not-equals>
						<property>/controls/switches/no-smoking-sign</property>
						<value>1</value>
					</not-equals>
					<not-equals>
						<property>/controls/switches/seatbelt-sign</property>
						<value>1</value>
					</not-equals>
				</and>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>/controls/switches/no-smoking-sign</property>
				<value>2</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/controls/switches/seatbelt-sign</property>
				<value>0</value>
			</binding>
		</item>
		<item>
			<name>ATIS / weather report</name>
			<value>CHECK</value>
			<binding>
				<command>dialog-show</command>
				<dialog-name>weather</dialog-name>
			</binding>
		</item>
		<item>
			<name>Altimeter</name>
			<value>SET</value>
			<marker>
				<x-m>-13.9790</x-m>
				<y-m>-0.7223</y-m>
				<z-m>-1.0637</z-m>
				<scale>1.8760</scale>
			</marker>
		</item>
		<item>
			<name>Clearance</name>
			<value>OBTAIN</value>
		</item>

		<item>
			<name>Doors (see/click EICAS doors page)</name>
			<value>CLOSE</value>
			<condition>
				<and>
					<not><property>/sim/model/door-positions/aft-cargo/position-norm</property></not>
					<not><property>/sim/model/door-positions/avionic/position-norm</property></not>
					<not><property>/sim/model/door-positions/ctr-cargo/position-norm</property></not>
					<not><property>/sim/model/door-positions/emer-l1/position-norm</property></not>
					<not><property>/sim/model/door-positions/emer-l2/position-norm</property></not>
					<not><property>/sim/model/door-positions/emer-r1/position-norm</property></not>
					<not><property>/sim/model/door-positions/emer-r2/position-norm</property></not>
					<not><property>/sim/model/door-positions/fwd-cargo/position-norm</property></not>
					<not><property>/sim/model/door-positions/pax-left/position-norm</property></not>
					<not><property>/sim/model/door-positions/pax-right/position-norm</property></not>
				</and>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>5</value>
			</binding>		
			<binding>
				<command>nasal</command>
				<script>CRJ700.doors.close();</script>
			</binding>
		</item>
		<item>
			<name>Before pushback: APU</name>
			<value>ON</value>
		</item>		
	</page>
</PropertyList>
