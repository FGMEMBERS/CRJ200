<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>
	<name>eicas</name>
    <layout>vbox</layout>
	<x>-100</x>
	<y>1</y>	
	<group>
		<layout>hbox</layout>
		<text>
			<label>EICAS</label>
				<color>
					<red>0.9</red>
					<green>0.9</green>
					<blue>0.1</blue>
					<alpha>1</alpha>
				</color>
		</text>
		<button>
			<legend>PRI</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>1</value>
			</binding>
		</button>
		<button>
			<legend>STAT</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>2</value>
			</binding>
		</button>
		<button>
			<legend>ECS</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<enabled>false</enabled>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>3</value>
			</binding>
		</button>
		<button>
			<legend>HYD</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>4</value>
			</binding>
		</button>
		<button>
			<legend>AC/DC</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<binding>
				<command>property-cycle</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>5</value>
				<value>6</value>
			</binding>
		</button>
		<button>
			<legend>FUEL</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<enabled>false</enabled>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>7</value>
			</binding>
		</button>
		<button>
			<legend>F/CTL</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<enabled>false</enabled>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>8</value>
			</binding>
		</button>
		<button>
			<legend>A/ICE</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<enabled>false</enabled>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>9</value>
			</binding>
		</button>
		<button>
			<legend>DOORS</legend>
			<pref-height>22</pref-height>
			<pref-width>55</pref-width>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/eicas[1]/page</property>
				<value>10</value>
			</binding>
		</button>
		<button>
			<pref-width>16</pref-width>
			<pref-height>16</pref-height>
			<legend>X</legend>
			<keynum>27</keynum>
			<border>2</border>
			<binding>
				<command>dialog-close</command>
			</binding>
		</button>
	</group>
</PropertyList>
