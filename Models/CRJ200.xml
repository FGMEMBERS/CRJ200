    <animation>
        <type>pick</type>
        <object-name>doorFL</object-name>
        <object-name>doorFLint</object-name>
        <object-name>stairs</object-name>
        <action>
            <button>0</button>
            <repeatable type="bool">false</repeatable>
            <binding>
                <command>nasal</command>
                <script>
                    CRJ200.doors.pax_left.toggle();
                </script>
            </binding>
        </action>
    </animation>
	<animation>
		<type>rotate</type>
		<object-name>doorFL</object-name>
