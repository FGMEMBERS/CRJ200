		<factor>163</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-11.062</x-m>
			<y-m>-1.38575</y-m>
			<z-m>-0.00395</z-m>
		</center>
	</animation>
