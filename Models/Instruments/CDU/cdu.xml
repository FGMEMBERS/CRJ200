<?xml version="1.0" encoding="UTF-8" ?>

<!-- Bombardier CRJ700 series -->
<!-- Control display unit -->

<PropertyList>

	<path>cdu.ac</path>

	<params>
		<lightmap-texture-path>Aircraft/Generic/generic-lightmap.png</lightmap-texture-path>
	</params>
	<small-text-size type="float">0.006</small-text-size>
	<large-text-size type="float">0.008</large-text-size>

	<!-- Visible only within 1/2 km -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>500</max-m>
	</animation>

	<animation>
		<type>material</type>
		<object-name>Keys</object-name>
		<condition>
			<greater-than-equals>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
			</greater-than-equals>
		</condition>
		<emission>
			<red>0.4</red>
			<green>0.384</green>
			<blue>0.171</blue>
		</emission>
	</animation>

	<!-- Display lines -->
	<model>
		<name>Line title</name>
		<path>Aircraft/CRJ700-family/Models/Instruments/CDU/display-text-value.xml</path>
		<overlay>
			<params>
				<property>instrumentation/cdu/display/title/value</property>
				<format>%s</format>
				<character-size alias="../../../../large-text-size" />
				<alignment>center-center</alignment>
				<color>
					<red-prop>instrumentation/cdu/display/title/color-red-norm</red-prop>
					<green-prop>instrumentation/cdu/display/title/color-green-norm</green-prop>
					<blue-prop>instrumentation/cdu/display/title/color-blue-norm</blue-prop>
				</color>
			</params>
		</overlay>
		<offsets>
			<x-m>0.0001</x-m>
			<y-m>-0.006</y-m>
			<z-m>0.105</z-m>
		</offsets>
	</model>
